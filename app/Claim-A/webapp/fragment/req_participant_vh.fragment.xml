<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <TableSelectDialog
        title="Select Employee"
        noDataText="No Employees Found"
        search=".onValueHelpSearch"
        confirm=".onValueHelpConfirm"
        cancel=".onValueHelpCancel"
        items="{/ZEMP_MASTER}">
        <columns>
            <Column><Text text="EEID" /></Column>
            <Column><Text text="Name" /></Column>
            <Column><Text text="CC" /></Column>
            <Column><Text text="Role" /></Column>
            <Column><Text text="Position" /></Column>
        </columns>
        <items>
            <ColumnListItem type="Active">
                <cells>
                    <Text text="{EEID}" />
                    <Text text="{NAME}" />
                    <Text text="{CC}" />
                    <Text text="{ROLE}" />
                    <Text text="{POSITION_NAME}" />
                </cells>
            </ColumnListItem>
        </items>
    </TableSelectDialog>
</core:FragmentDefinition>