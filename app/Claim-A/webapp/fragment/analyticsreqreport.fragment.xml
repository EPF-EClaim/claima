<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:f="sap.f"
    xmlns:mdc="sap.ui.mdc"
    xmlns:mdct="sap.ui.mdc.table"
>
    <!-- Root must be a Page so NavContainer can navigate to it -->
    <Page
        id="analyticsReqReportPage"
        title="{i18n>ana_poptitle}"
        showNavButton="true"
        navButtonPress=".onBackFromAnalyticsClaimReport"
    >
        <content>
            <f:DynamicPage id="analyticsReqDynPage">
                <f:content>
                    <mdc:Table
                        id="analyticsreqtab"
                        enableExport="true"
                    >
                        <mdct:Column
                            propertyKey="employee"
                            header="{i18n>employee_name}"
                        >
                            <Text text="{mountains>name} ({mountains>range})" />
                        </mdct:Column>
                        <mdct:Column
                            propertyKey="personal_grade"
                            header="{i18n>personal_grade}"
                        >
                            <Text text="{mountains>range}" />
                        </mdct:Column>
                        <mdct:Column
                            propertyKey="dept"
                            header="{i18n>department_id}"
                        >
                            <Text text="{mountains>first_ascent}" />
                        </mdct:Column>
                        <mdct:Column
                            propertyKey="dept_name"
                            header="{i18n>department_name}"
                        >
                            <Text text="{mountains>height} m" />
                        </mdct:Column>
                        <mdct:Column
                            propertyKey="unit"
                            header="{i18n>unit}"
                        >
                            <Text text="{mountains>countries}" />
                        </mdct:Column>
                        <mdct:Column
                            propertyKey="position"
                            header="{i18n>position}"
                        >
                            <Text text="{mountains>parent_mountain}" />
                        </mdct:Column>
                        <mdct:Column
                            propertyKey="employee_id"
                            header="{i18n>employee_id}"
                        >

                        <mdct:Column
                            propertyKey="claim_type"
                            header="{i18n>claim_type}"
                        >
                            <Text text="{mountains>parent_mountain}" />
                        </mdct:Column>
                            <Text text="{mountains>parent_mountain}" />
                        </mdct:Column>
                                               
                        <mdct:Column
                            propertyKey="preappreq_id"
                            header="{i18n>preappreq_id}"
                        >
                            <Text text="{mountains>parent_mountain}" />
                        </mdct:Column>
                        <mdct:Column
                            propertyKey="preappreq_date"
                            header="{i18n>preappreq_date}"
                        >
                            <Text text="{mountains>parent_mountain}" />
                        </mdct:Column>
                        <mdct:Column
                            propertyKey="req_rtype"
                            header="{i18n>req_rtype}"
                        >
                            <Text text="{mountains>parent_mountain}" />
                        </mdct:Column>
                        <mdct:Column
                            propertyKey="submitted_date"
                            header="{i18n>submitted_date}"
                        >
                            <Text text="{mountains>parent_mountain}" />
                        </mdct:Column>
                        <mdct:Column
                            propertyKey="approval1"
                            header="{i18n>approval1}"
                        >
                            <Text text="{mountains>parent_mountain}" />
                        </mdct:Column>
                        <mdct:Column
                            propertyKey="approval2"
                            header="{i18n>approval2}"
                        >
                            <Text text="{mountains>parent_mountain}" />
                        </mdct:Column>
                        <mdct:Column
                            propertyKey="approval3"
                            header="{i18n>approval3}"
                        >
                            <Text text="{mountains>parent_mountain}" />
                        </mdct:Column>
                        <mdct:Column
                            propertyKey="approval4"
                            header="{i18n>approval4}"
                        >
                            <Text text="{mountains>parent_mountain}" />
                        </mdct:Column>
                        <mdct:Column
                            propertyKey="approval5"
                            header="{i18n>approval5}"
                        >
                            <Text text="{mountains>parent_mountain}" />
                        </mdct:Column>
                        <mdct:Column
                            propertyKey="final_app_date"
                            header="{i18n>final_app_date}"
                        >
                            <Text text="{mountains>parent_mountain}" />
                        </mdct:Column>

                        <mdct:Column
                            propertyKey="last_send_date"
                            header="{i18n>last_send_date}"
                        >
                            <Text text="{mountains>parent_mountain}" />
                        </mdct:Column>

                        <mdct:Column
                            propertyKey="days_approved"
                            header="{i18n>days_approved}"
                        >
                            <Text text="{mountains>parent_mountain}" />
                        </mdct:Column>
                        <mdct:Column
                            propertyKey="cash_adv_recei_date"
                            header="{i18n>cash_adv_recei_date}"
                        >
                            <Text text="{mountains>parent_mountain}" />
                        </mdct:Column>
                        <mdct:Column
                            propertyKey="payment_date"
                            header="{i18n>payment_date}"
                        >
                            <Text text="{mountains>parent_mountain}" />
                        </mdct:Column>

                        <mdct:Column
                            propertyKey="pre_app_status"
                            header="{i18n>pre_app_status}"
                        >
                            <Text text="{mountains>parent_mountain}" />
                        </mdct:Column>

                        <mdct:Column
                            propertyKey="cc_code"
                            header="{i18n>cc_code}"
                        >
                            <Text text="{mountains>parent_mountain}" />
                        </mdct:Column>
                        <mdct:Column
                            propertyKey="cc_desc"
                            header="{i18n>cc_desc}"
                        >
                            <Text text="{mountains>parent_mountain}" />
                        </mdct:Column>
                        <mdct:Column
                            propertyKey="alt_cc_code"
                            header="{i18n>alt_cc_code}"
                        >
                            <Text text="{mountains>parent_mountain}" />
                        </mdct:Column>
                        <mdct:Column
                            propertyKey="alt_cc_desc"
                            header="{i18n>alt_cc_desc}"
                        >
                            <Text text="{mountains>parent_mountain}" />
                        </mdct:Column>
                        <mdct:Column
                            propertyKey="gl_acct"
                            header="{i18n>gl_acct}"
                        >
                            <Text text="{mountains>parent_mountain}" />
                        </mdct:Column>
                        <mdct:Column
                            propertyKey="code_mat"
                            header="{i18n>code_mat}"
                        >
                            <Text text="{mountains>parent_mountain}" />
                        </mdct:Column>
                        <mdct:Column
                            propertyKey="gl_acct"
                            header="{i18n>gl_acct}"
                        >
                            <Text text="{mountains>parent_mountain}" />
                        </mdct:Column>
                        <mdct:Column
                            propertyKey="total_req_amt"
                            header="{i18n>total_req_amt}"
                        >
                            <Text text="{mountains>parent_mountain}" />
                        </mdct:Column>
                        <mdct:Column
                            propertyKey="cash_adv"
                            header="{i18n>cash_adv}"
                        >
                            <Text text="{mountains>parent_mountain}" />
                        </mdct:Column>
        
                        <mdct:Column
                            propertyKey="purpose"
                            header="{i18n>purpose}"
                        >
                            <Text text="{mountains>parent_mountain}" />
                        </mdct:Column>
                        <mdct:Column
                            propertyKey="req_i_remarks"
                            header="{i18n>req_i_remarks}"
                        >
                            <Text text="{mountains>parent_mountain}" />
                        </mdct:Column>

                        <mdct:Column
                            propertyKey="req_tripstartdate"
                            header="{i18n>req_tripstartdate}"
                        >
                            <Text text="{mountains>parent_mountain}" />
                        </mdct:Column>
                        <mdct:Column
                            propertyKey="req_tripenddate"
                            header="{i18n>req_tripenddate}"
                        >
                            <Text text="{mountains>parent_mountain}" />
                        </mdct:Column>
                        <mdct:Column
                            propertyKey="req_eventstartdate"
                            header="{i18n>req_eventstartdate}"
                        >
                            <Text text="{mountains>parent_mountain}" />
                        </mdct:Column>
                        <mdct:Column
                            propertyKey="req_eventenddate"
                            header="{i18n>req_eventenddate}"
                        >
                            <Text text="{mountains>parent_mountain}" />
                        </mdct:Column>
                        <mdct:Column
                            propertyKey="location"
                            header="{i18n>location}"
                        >
                            <Text text="{mountains>parent_mountain}" />
                        </mdct:Column>
                        <mdct:Column
                            propertyKey="req_transport"
                            header="{i18n>req_transport}"
                        >
                            <Text text="{mountains>parent_mountain}" />
                        </mdct:Column>                       
                    </mdc:Table>
                </f:content>
            </f:DynamicPage>
        </content>

        <footer>
            <Toolbar
                design="Solid"
                class="sapUiTinyMargin"
            >
                <ToolbarSpacer />

                <Button
                    id="btnExportAnalytics_req"
                    text="{i18n>export}"
                    type="Emphasized"
                    press=".onExportAnalyticsReport"
                />

                <Button
                    id="btnCloseAnalytics_req"
                    text="{i18n>close}"
                    press=".onBackFromAnalyticsClaimReport"
                />
            </Toolbar>
        </footer>
    </Page>
</core:FragmentDefinition>
