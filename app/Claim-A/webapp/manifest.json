{
  "_version": "1.81.0",
  "sap.app": {
    "id": "claima",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.20.0",
      "toolsId": "5e53ce21-6b98-4c53-9e82-de44fa4c3b3b"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/EmployeeSrv/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    },
    "fullWidth": true
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.143.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.ui.table": {},
        "sap.ui.unified": {},
        "sap.fe.templates": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "employee": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "manifests": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "model/data.json"
      },
      "current": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "model/data2.json"
      },
      "input": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "model/data2.json"
      },
      "mockdata": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "model/mockdata.json"
      },
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "claima.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "pageContainer",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "",
        "async": true
      },
      "routes": [
        {
          "name": "RouteMain",
          "pattern": ":?query:",
          "target": [
            "TargetMain"
          ]
        },
        {
          "pattern": "Dashboard",
          "name": "Dashboard",
          "target": "TargetDashboard"
        }, 
         {
          "pattern": "ZCLAIM_PURPOSE",
          "name": "ZCLAIM_PURPOSE",
          "target": [
            "ZCLAIM_PURPOSE"
          ]
        },
        {
          "pattern": "ZRISK",
          "name": "ZRISK",
          "target": [
            "ZRISK"
          ]
        }, 
        {
          "pattern": "MainConfiguration",
          "name": "MainConfiguration",
          "target":[
            "MainConfiguration"
          ]
        }    
      ],
      "targets": {
        "TargetMain": {
          "id": "Main",
          "name": "Main"
        },
        "TargetDashboard": {
          "id": "Dashboard",
          "name": "App"
        },
        "ZCLAIM_PURPOSE": {
          "type": "Component",
          "id": "ZCLAIM_PURPOSE",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/ZCLAIM_PURPOSE",
              "variantManagement": "None",
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "type": "ResponsiveTable",
                    "createMode": "inline",
                    "initialLoad": "Enabled",
                    "enableExport": true,
                    "enableMassEdit": {
                      "visibleFields": "CLAIM_PURPOSE_ID,CLAIM_PURPOSE_DESC",
                      "operationGroupingMode": "Isolated"
                    },
                    "creationMode": {
                      "name": "CreationDialog",
                      "creationFields": "CLAIM_PURPOSE_ID,CLAIM_PURPOSE_DESC"
                    },
                    "selectAll": false,
                    "selectionMode": "Single",
                    "personalization": {
                      "column": false
                    },
                    "quickVariantSelection": {
                      "hideTableTitle": true
                    }
                  }
                }
              },
              "hideFilterBar": true,
              "initialLoad": "Enabled",
              "navigation": {}
            }
          }
        }, 
        "ZRISK": {
          "type": "Component",
          "id": "ZRISK",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/ZRISK",
              "variantManagement": "None",
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "type": "ResponsiveTable",
                    "createMode": "inline",
                    "initialLoad": "Enabled",
                    "enableExport": true,
                    "selectAll": false,
                    "selectionMode": "Single",
                    "personalization": {
                      "column": false
                    },
                    "quickVariantSelection": {
                      "hideTableTitle": true
                    }
                  }
                }
              },
              "hideFilterBar": true,
              "initialLoad": "Enabled",
              "navigation": {}
            }
          }
        },
        "MainConfiguration":{
          "id": "MainConfiguration", 
          "name": "MainConfiguration", 
          "type": "View"
        }
      }
    },
    "extends": {
      "extensions": {
        "sap.ui.controllerExtensions": {
          "sap.fe.templates.ListReport.ListReportController" : {
            "controllerName": "claima.controller.configuration"
          }
        }
      }
    },
    "rootView": {
      "viewName": "claima.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    },
    "config": {
      "sample": {
        "stretch": true,
        "files": [
          "App.view.xml",
          "App.controller.js",
          "manifest.json",
          "model/data.json"
        ]
      }
    }
  },
  "sap.fiori": {
     "registrationIds": [],
        "archeType": "transactional"
  }
}