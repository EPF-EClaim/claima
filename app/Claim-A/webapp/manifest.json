{
  "_version": "1.81.0",
  "sap.app": {
    "id": "claima",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.20.0",
      "toolsId": "5e53ce21-6b98-4c53-9e82-de44fa4c3b3b"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/EmployeeSrv/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    }
  },  
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    },
    "fullWidth": true
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.143.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.ui.table": {},
        "sap.ui.unified": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {   
      "employee": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "manifests": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "model/data.json"
      },
      "current": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "model/data2.json"
      },
      "input": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "model/data2.json"
      },
      "mockdata": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "model/mockdata.json"
      },
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "claima.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "claima.view",
        "async": true,
        "viewPath": "claima.view"
      },
      "routes": [
        {
          "name": "RouteMain",
          "pattern": ":?query:",
          "target": [
            "TargetMain"
          ]
        }, 
        {
          "pattern": "Dashboard",
          "name": "Dashboard",
          "target": "TargetDashboard"
        }
      ],
      "targets": {
        "TargetMain": {
          "id": "Main",
          "name": "Main"
        }, 
        "TargetDashboard": {
          "viewId": "Dashboard",
          "viewName": "App"
        }
      }
    },
    "rootView": {
      "viewName": "claima.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    },
    "config": {
      "sample": {
        "stretch": true,
        "files": [
          "App.view.xml",
          "App.controller.js",
          "manifest.json",
          "model/data.json"
        ]
      }
    }
  }
}