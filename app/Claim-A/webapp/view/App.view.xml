<mvc:View
    controllerName="claima.controller.App"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:tnt="sap.tnt"
    xmlns:cards="sap.f.cards"
    xmlns:f="sap.f"
    xmlns:l="sap.ui.layout"
    xmlns:cssgrid="sap.ui.layout.cssgrid"
    xmlns:w="sap.ui.integration.widgets"
    xmlns:card="sap.f.cards"
    xmlns:core="sap.ui.core"
    xmlns:Icon="sap.ui.core.Icon"
    xmlns:ShellBar="sap.f.ShellBar"
    xmlns:layout="sap.ui.layout"
    displayBlock="true"
    height="100%"
    class="sapUiResponsiveMargin"
>
    <tnt:ToolPage id="toolPage">
        <tnt:header>
            <f:ShellBar
                showMenuButton="true"
                homeIcon="./resources/sap/ui/documentation/sdk/images/logo_sap.png"
                menuButtonPressed="onMenuButtonPress"
            />
        </tnt:header>
        <tnt:sideContent>
            <!-- <core:Fragment
					fragmentName="view.navigation"
					type="XML" /> -->
            <tnt:SideNavigation
                expanded="true"
                itemPress=".onNavItemPress"
                selectedKey="dashboard"
                itemSelect=".onNavItemSelect"
            >
                <tnt:NavigationList>
                    <tnt:NavigationListItem
                        key="dashboard"
                        text="{i18n>nav_dashboard}"
                        icon="sap-icon://home"
                        enabled="true"
                        expanded="false"
                    />
                    <tnt:NavigationListItem
                        key="nav_createreport"
                        text="{i18n>nav_createreport}"
                        icon="sap-icon://add"
                        enabled="true"
                        expanded="false"
                    />
                    <tnt:NavigationListItem
                        key="createrequest"
                        text="{i18n>nav_createrequest}"
                        icon="sap-icon://sys-add"
                        enabled="true"
                        expanded="false"
                    />
                    <tnt:NavigationListItem
                        key="myreport"
                        text="{i18n>nav_myreport}"
                        icon="sap-icon://activity-items"
                        enabled="true"
                        expanded="false"
                    />
                    <tnt:NavigationListItem
                        key="myrequest"
                        text="{i18n>nav_myrequest}"
                        icon="sap-icon://document-text"
                        enabled="true"
                        expanded="false"
                    />
                    <tnt:NavigationListItem
                        key="approval"
                        text="{i18n>nav_approval}"
                        icon="sap-icon://sys-enter"
                        enabled="true"
                        expanded="false"
                    />
                    <tnt:NavigationListItem
                        key="analytics"
                        text="{i18n>nav_analytics}"
                        icon="sap-icon://business-objects-experience"
                        enabled="true"
                        expanded="false"
                    />
                    <tnt:NavigationListItem
                        key="config"
                        text="{i18n>nav_config}"
                        icon="sap-icon://action-settings"
                        enabled="true"
                        expanded="false"
                    />
                </tnt:NavigationList>
            </tnt:SideNavigation>
        </tnt:sideContent>
        <tnt:mainContents>
            <NavContainer
                id="pageContainer"
                initialPage="dashboard"
            >
                <pages>
                    <ScrollContainer
                        id="dashboard"
                        horizontal="false"
                        vertical="true"
                        height="100%"
                        class="sapUiContentPadding"
                    >
                        <VBox>
                            <HBox>
                                <VBox
                                    class="sapUiSmallMargin"
                                    justifyContent="SpaceBetween"
                                >
                                    <f:GridContainer>
                                        <f:layout>
                                            <f:GridContainerSettings
                                                rowSize="5rem"
                                                columnSize="20rem"
                                                gap="1rem"
                                            />
                                        </f:layout>
                                        <w:Card
                                            manifest="{manifests>/expensereport}"
                                            action="onAction"
                                        />
                                    </f:GridContainer>
                                </VBox>
                                <VBox
                                    class="sapUiSmallMargin"
                                    justifyContent="SpaceBetween"
                                >
                                    <f:GridContainer>
                                        <f:layout>
                                            <f:GridContainerSettings
                                                rowSize="5rem"
                                                columnSize="20rem"
                                                gap="1rem"
                                            />
                                        </f:layout>
                                        <w:Card
                                            manifest="{manifests>/myrequest}"
                                            action="onAction"
                                        />
                                    </f:GridContainer>
                                </VBox>
                                <VBox
                                    class="sapUiSmallMargin"
                                    justifyContent="SpaceBetween"
                                >
                                    <f:GridContainer>
                                        <f:layout>
                                            <f:GridContainerSettings
                                                rowSize="5rem"
                                                columnSize="20rem"
                                                gap="1rem"
                                            />
                                        </f:layout>
                                        <w:Card
                                            manifest="{manifests>/approval}"
                                            action="onAction"
                                        />
                                    </f:GridContainer>
                                </VBox>
                            </HBox>

                            <f:GridContainer>
                                <f:Card
                                    class="sapUiSmallMargin"
                                    width="90rem"
                                >
                                    <f:header>
                                        <cards:Header
                                            title="{i18n>myexpensesreport}"
                                            iconSrc="sap-icon://document-text"
                                        />
                                    </f:header>
                                    <f:content>
                                        <List
                                            class="sapUiSmallMarginBottom"
                                            showSeparators="None"
                                        />
                                    </f:content>
                                </f:Card>
                            </f:GridContainer>

                            <f:GridContainer>
                                <f:Card
                                    class="sapUiSmallMargin"
                                    width="90rem"
                                >
                                    <f:header>
                                        <cards:Header
                                            title="{i18n>myrequest}"
                                            iconSrc="sap-icon://activity-items"
                                        />
                                    </f:header>
                                    <f:content>
                                        <List
                                            class="sapUiSmallMarginBottom"
                                            showSeparators="None"
                                        />
                                    </f:content>
                                </f:Card>
                            </f:GridContainer>

                            <f:GridContainer>
                                <f:Card
                                    class="sapUiSmallMargin"
                                    width="90rem"
                                >
                                    <f:header>
                                        <cards:Header
                                            title="{i18n>approvals}"
                                            iconSrc="sap-icon://sys-enter"
                                        />
                                    </f:header>
                                    <f:content>
                                        <List
                                            class="sapUiSmallMarginBottom"
                                            showSeparators="None"
                                        />
                                    </f:content>
                                </f:Card>
                                <!-- </Panel> -->
                            </f:GridContainer>
                        </VBox>
                    </ScrollContainer>
                    <ScrollContainer
                        id="claim"
                        horizontal="false"
                        vertical="true"
                        height="100%"
                    >
                        <VBox class="sapUiResponsiveContentPadding">
                            <HBox class="sapUiResponsiveContentPadding">
                                <Title
                                    text="Claim"
                                    textAlign="Right"
                                    level="H3"
                                    titleStyle="H3"
                                    wrapping="true"
                                />
                            </HBox>
                            <HBox class="sapUiResponsiveContentPadding">
                                <Carousel
                                    id="carousel"
                                    ariaLabelledBy="carouselTitle"
                                    height="auto"
                                >
                                    <customLayout>
                                        <CarouselLayout visiblePagesCount="4" />
                                    </customLayout>
                                    <f:Card>
                                        <f:header>
                                            <cards:Header title="Draft Claim" />
                                        </f:header>
                                        <f:content>
                                            <l:VerticalLayout
                                                class="sapUiContentPadding"
                                                width="100%"
                                            >
                                                <l:BlockLayout>
                                                    <l:BlockLayoutRow>
                                                        <l:BlockLayoutCell
                                                            width="1"
                                                        >
                                                            <HBox
                                                                class="sapUiTinyMarginBottom"
                                                            >
                                                                <Label
                                                                    text="Claim ID:"
                                                                />
                                                            </HBox>
                                                            <HBox
                                                                class="sapUiTinyMarginBottom"
                                                            >
                                                                <Label
                                                                    text="Claim Type:"
                                                                />
                                                            </HBox>
                                                            <HBox
                                                                class="sapUiTinyMarginBottom"
                                                            >
                                                                <Label
                                                                    text="Details:"
                                                                />
                                                            </HBox>
                                                        </l:BlockLayoutCell>
                                                    </l:BlockLayoutRow>
                                                </l:BlockLayout>
                                            </l:VerticalLayout>
                                        </f:content>
                                    </f:Card>
                                </Carousel>
                            </HBox>
                            <HBox
                                class="sapUiResponsiveContentPadding"
                                id="content"
                            >
                                <VBox
                                    height="110px"
                                    class="sapUiSmallMargin"
                                    justifyContent="SpaceBetween"
                                >
                                    <f:GridContainer>
                                        <f:layout>
                                            <f:GridContainerSettings
                                                rowSize="20rem"
                                                columnSize="25rem"
                                                gap="3rem"
                                            />
                                        </f:layout>
                                        <w:Card
                                            manifest="{manifests>/object}"
                                            action="onAction"
                                        >
                                            <w:layoutData>
                                                <f:GridContainerItemLayoutData
                                                    minRows="4"
                                                    columns="3"
                                                />
                                            </w:layoutData>
                                        </w:Card>
                                    </f:GridContainer>
                                </VBox>
                                <VBox
                                    height="110px"
                                    class="sapUiSmallMargin"
                                    justifyContent="SpaceBetween"
                                >
                                    <f:GridContainer>
                                        <f:layout>
                                            <f:GridContainerSettings
                                                rowSize="20rem"
                                                columnSize="25rem"
                                                gap="1rem"
                                            />
                                        </f:layout>
                                        <w:Card
                                            manifest="{manifests>/preclaim}"
                                            action="onAction"
                                        >
                                            <w:layoutData>
                                                <f:GridContainerItemLayoutData
                                                    minRows="4"
                                                    columns="3"
                                                />
                                            </w:layoutData>
                                        </w:Card>
                                    </f:GridContainer>
                                </VBox>
                                <VBox
                                    height="110px"
                                    class="sapUiSmallMargin"
                                    justifyContent="SpaceBetween"
                                >
                                    <f:GridContainer>
                                        <f:layout>
                                            <f:GridContainerSettings
                                                rowSize="5rem"
                                                columnSize="25rem"
                                                gap="1rem"
                                            />
                                        </f:layout>
                                        <w:Card
                                            manifest="{manifests>/autoclaim}"
                                            action="onAction"
                                        >
                                            <w:layoutData>
                                                <f:GridContainerItemLayoutData
                                                    minRows="4"
                                                    columns="3"
                                                />
                                            </w:layoutData>
                                        </w:Card>
                                    </f:GridContainer>
                                </VBox>
                                <VBox
                                    height="110px"
                                    class="sapUiSmallMargin"
                                    justifyContent="SpaceBetween"
                                >
                                    <f:GridContainer>
                                        <f:layout>
                                            <f:GridContainerSettings
                                                rowSize="5rem"
                                                columnSize="25rem"
                                                gap="1rem"
                                            />
                                        </f:layout>
                                        <w:Card
                                            manifest="{manifests>/returnclaim}"
                                            action="onAction"
                                        >
                                            <w:layoutData>
                                                <f:GridContainerItemLayoutData
                                                    minRows="4"
                                                    columns="3"
                                                />
                                            </w:layoutData>
                                        </w:Card>
                                    </f:GridContainer>
                                </VBox>
                            </HBox>
                        </VBox>
                    </ScrollContainer>

                    <Page
                        id="expensereport"
                        showHeader="false"
                    >
                        <content>
                            <VBox id="main">
                                <core:Fragment
                                    fragmentName="claima.fragment.report"
                                    type="XML"
                                />
                                <VBox
                                    id="expensetypescr"
                                    visible="true"
                                >
                                    <core:Fragment
                                        fragmentName="claima.fragment.expensetype"
                                        type="XML"
                                    />
                                </VBox>
                                <VBox
                                    id="claimscr"
                                    visible="false"
                                >
                                    <core:Fragment
                                        fragmentName="claima.fragment.claimdetails"
                                        type="XML"
                                    />
                                </VBox>
                            </VBox>
                        </content>
                        <footer>
                            <Toolbar>
                                <ToolbarSpacer />
                                <Button
                                    id="cancelbtn"
                                    text="Cancel"
                                    press="onPressCancel"
                                    enabled="true"
                                    visible="false"
                                />
                                <Button
                                    id="savebtn"
                                    text="Save"
                                    press="onPressSave"
                                    enabled="true"
                                    visible="false"
                                />
                                <Button
                                    id="backbtn"
                                    text="Back"
                                    press="onPressBack"
                                    enabled="true"
                                />
                                <Button
                                    id="draft"
                                    text="Save Draft"
                                    press="onPress"
                                    enabled="false"
                                />

                                <Button
                                    id="delete"
                                    text="Delete Report"
                                    press="onPress"
                                    enabled="false"
                                />

                                <Button
                                    id="submit"
                                    text="Submit Report"
                                    press="onPress"
                                    enabled="false"
                                />
                            </Toolbar>
                        </footer>
                    </Page>
                    <ScrollContainer
                        id="config"
                        horizontal="false"
                        vertical="true"
                        height="100%"
                        class="sapUiContentPadding"
                    />
                    <!-- ================= Configuration Page ================= -->
                    <Page
                        id="configurationPage"
                        title="{i18n>configtitle}"
                        navButtonPress="onPressBack">
                        <content>
                            <ScrollContainer
                                vertical="true"
                                horizontal="false"
                                height="100%"
                                class="sapUiContentPadding">    
                                <f:GridContainer>
                                    <f:layout>
                                        <f:GridContainerSettings
                                            columnSize="16rem"
                                            rowSize="auto"
                                            gap="1rem"
                                        />
                                    </f:layout>

                                    <!-- ZCLAIM_PURPOSE -->
                                    <f:Card
                                        class="sapUiSmallMargin"
                                        width="15rem"
                                        press="onOpenConfigTable"
                                    >
                                       <f:customData>
                                            <core:CustomData
                                                key="id"
                                                value="ZCLAIM_PURPOSE"
                                            />
                                        </f:customData>
                                        <f:header>
                                            <cards:Header
                                                title="{i18n>conf1}"
                                            />
                                        </f:header>
                                    </f:Card>

                                    <!-- Configuration Table 2 -->
                                    <f:Card
                                        class="sapUiSmallMargin"
                                        width="15rem"
                                        press="onOpenConfigTable"
                                    >
                                       <f:customData>
                                            <core:CustomData
                                                key="id"
                                                value="ConfigurationTable2"
                                            />
                                        </f:customData>
                                        <f:header>
                                            <cards:Header
                                                title="{i18n>conf2}"
                                            />
                                        </f:header>
                                    </f:Card>

                                    <!-- Configuration Table 3 -->
                                    <f:Card
                                        class="sapUiSmallMargin"
                                        width="15rem"
                                        press="onOpenConfigTable"
                                    >
                                       <f:customData>
                                            <core:CustomData
                                                key="id"
                                                value="ConfigurationTable3"
                                            />
                                        </f:customData>
                                        <f:header>
                                            <cards:Header
                                                title="{i18n>conf3}"
                                            />
                                        </f:header>
                                    </f:Card>

                                    <!-- Configuration Table 4 -->
                                    <f:Card
                                        class="sapUiSmallMargin"
                                        width="15rem"
                                        press="onOpenConfigTable"
                                    >
                                        <f:customData>
                                            <core:CustomData
                                                key="id"
                                                value="ConfigurationTable4"
                                            />
                                        </f:customData>
                                        <f:header>
                                            <cards:Header
                                                title="{i18n>conf4}"
                                            />
                                        </f:header>
                                    </f:Card>
                                </f:GridContainer>
                            </ScrollContainer>
                        </content>
                    </Page> <!-- ================= Configuration Page ================= -->
                  
                    <ScrollContainer
                        id="analytics"
                        horizontal="false"
                        vertical="true"
                        height="100%"
                        class="sapUiContentPadding"
                    >
                        <Text text="test text" />
                    </ScrollContainer>
					<NavContainer
						id="new_request"
						class="sapUiContentPadding"
						height="100%"
						width="100%">
						
						<mvc:XMLView
							id="request_form_view"
							viewName="claima.view.RequestForm" />

					</NavContainer>
                </pages>
                <!-- === My Request Page (added) === -->
                <Page id="myrequest" showHeader="false" class="sapUiNoContentPadding">
                    <content>
                        <core:Fragment
                            fragmentName="claima.fragment.myrequestform"
                            type="XML"
                        />
                    </content>
                </Page>
                <!-- === My Request Report (added) === -->
                <Page id="myreport" showHeader="false" class="sapUiNoContentPadding">
                    <content>
                        <core:Fragment
                            fragmentName="claima.fragment.myrequestreport"
                            type="XML"
                        />
                    </content>
                </Page>

            </NavContainer>
        </tnt:mainContents>
    </tnt:ToolPage>
</mvc:View>
